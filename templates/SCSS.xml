<templateSet group="SCSS">
  <template name="css" value=".$COMPONENT$ {&#10;  $END$&#10;}" description="Parent scss selector" toReformat="true" toShortenFQNames="true">
    <variable name="COMPONENT" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="cssm" value=".$COMPONENT$ {&#10;  $END$&#10;  @media #{$tablet} {&#10;  &#10;  }&#10;  &#10;  @media #{$tablet-landscape} {&#10;  &#10;  }&#10;  &#10;  @media #{$desktop} {&#10;  &#10;  }&#10;}" description="Parent scss selector with media-queries" toReformat="true" toShortenFQNames="true">
    <variable name="COMPONENT" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="im" value="@import &quot;$FILE$&quot;;" description="Import scss file" toReformat="true" toShortenFQNames="true">
    <variable name="FILE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="memo" value="@media #{$mobile-only} {&#10;  $START$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="met" value="@media #{$tablet} {&#10;  $START$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="meto" value="@media #{$tablet-only} {&#10;  $START$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="metl" value="@media #{$tablet-landscape} {&#10;  $START$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="med" value="@media #{$desktop} {&#10;  $START$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
</templateSet>