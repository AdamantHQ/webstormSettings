<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Extend Selection">
        <Item>
          <IdeaActionID>EditorSelectWord</IdeaActionID>
          <ShortCut>'⌥↑' or '⌥Wheel down'</ShortCut>
          <Description>Extend Selection</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'AttributePicker'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'AttributePicker'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'PaymentToolbar.js'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'PaymentToolbar.js'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show History">
        <Item>
          <IdeaActionID>LocalHistory.ShowHistory</IdeaActionID>
          <ShortCut>'⌃⇧H'</ShortCut>
          <Description>Show History</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'AttributePicker'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'AttributePicker'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>15</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'⇧⌘B' or '⌃⇧B' or '⇧⌘Button1 Click' or '⇧Button2 Click'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>21</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>